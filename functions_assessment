# 1

def number_five():
    # returns the number
    return print("5")

number_five()

print()

def useful():
    # returns anything it's given :o
    anything = input("Enter anything: ")
    return print(anything)
useful()

print()

def add_two():
    # takes a number and adds two!!!
    num = int(input("Enter a number: "))
    num += 2
    return print(num)

add_two()

print()

