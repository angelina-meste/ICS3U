# 3    
    
def times_two(num: int) -> int:
    return num * 2

results = times_two(5)
print(results)

print()

# 4

def greeting(name: str) -> str:
    return f"Hello, {name}"

results = greeting("lina")
print(results)

print()

# 5

from typing import List

def all(nums: List[int]) -> List[int]:
    positive = []
    i = 0
    while i < len(nums):
        if nums[i] >= 0:
            positive.append(nums[i])
        i += 1

    return positive

results = all([1, 2, -1, 6])
print(results)
